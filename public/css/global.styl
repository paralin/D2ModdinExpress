body, html
  width: 100%
  height: 100%
  padding: 0

.tileTitle
  text-align center
  font-weight bold
  margin-top 5px
.container
  width:100%
  height:100%
  margin:0
  padding:0
.btn
  border-radius:0
.entryPanel
  width: 400px
  padding-bottom:10px
  margin-left: auto;
  margin-right: auto;
  margin-top: 20px;
.modThumbnail
  width: 250px
  height: 70px
  max-width: 300px
  max-height: 80px
  padding: 10px
  border-color: rgb(230, 230, 230);
  border-width: 1px;
  border-style: solid;
  text-align: left
  cursor: pointer
.modThumbnail .thumbDetail
  width: 170px
  height: 50px;
  float: right
.modThumbnail .thumbDetail p
  font-size: 12px
  margin: 0
  line-height: 15px
